[32m+[m[32mimport { SagaIterator } from "redux-saga"[m
[32m+[m[32mimport { call, put, take } from "redux-saga/effects"[m
[32m+[m[32mimport * as actions from "./../../store/actions"[m
[32m+[m[32mimport { myFetch } from "./../../store/apiClient"[m
[32m+[m
[32m+[m[32mconst query = `[m
[32m+[m[32m  query log($login: String!, $password: String!) {[m
[32m+[m[32m    login(login: $login, password: $password)[m
[32m+[m[32m  }[m
[32m+[m[32m`[m
[32m+[m[32mconst mutation = `[m
[32m+[m[32m  mutation createUser($login: String!, $password: String!){[m
[32m+[m[32m    createUser(login: $login, password: $password){[m
[32m+[m[32m        login[m
[32m+[m[32m        _id[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m`[m
[32m+[m
[32m+[m[32mexport function* authSaga(): SagaIterator {[m
[32m+[m[32m  while (true) {[m
[32m+[m[32m    const getToken = () => localStorage.getItem("token")[m
[32m+[m[32m    const authData = yield call(getToken)[m
[32m+[m[32m    if (authData) {[m
[32m+[m[32m      yield put(actions.authSuccess(authData))[m
[32m+[m[32m    } else {[m
[32m+[m[32m      try {[m
[32m+[m[32m        const action = yield take(actions.authRequest)[m
[32m+[m[32m        const { login: token } = yield call(myFetch, query, {[m
[32m+[m[32m          login: action.payload.login,[m
[32m+[m[32m          password: action.payload.password,[m
[32m+[m[32m        })[m
[32m+[m[32m        localStorage.setItem("token", token)[m
[32m+[m[41m        [m
[32m+[m[32m        yield put(actions.authSuccess(token))[m
[32m+[m[32m      } catch (e) {[m
[32m+[m[32m        console.error("!!! function*authSaga -> error", e)[m
[32m+[m[32m        yield put(actions.authFailure(e))[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    yield take(actions.logout)[m
[32m+[m[32m    yield put(actions.logout())[m
[32m+[m[32m    localStorage.removeItem("token")[m
[32m+[m[32m    localStorage.removeItem("setTracksTrue")[m
[32m+[m[32m    localStorage.removeItem("playlistPageLength")[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mexport function* regSaga(): SagaIterator {[m
[32m+[m[32m  while (true) {[m
[32m+[m[32m    const regAction = yield take(actions.regRequest)[m
[32m+[m
[32m+[m[32m    yield call(myFetch, mutation, {[m
[32m+[m[32m      login: regAction.payload.login,[m
[32m+[m[32m      password: regAction.payload.password,[m
[32m+[m[32m    })[m
[32m+[m
[32m+[m[32m    const { login: token } = yield call(myFetch, query, {[m
[32m+[m[32m      login: regAction.payload.login,[m
[32m+[m[32m      password: regAction.payload.password,[m
[32m+[m[32m    })[m
[32m+[m[32m    localStorage.setItem("token", token)[m
[32m+[m[32m    yield put(actions.authSuccess(token))[m
[32m+[m[32m  }[m
[32m+[m[32m}[m

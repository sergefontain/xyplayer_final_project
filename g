[32m+[m[32mimport { getType } from "typesafe-actions"[m
[32m+[m[32mimport * as actions from "../actions"[m
[32m+[m[32mimport { MainAction, MainState } from "./types"[m
[32m+[m
[32m+[m[32mconst initialState: MainState = {[m
[32m+[m[32m  playlists: null,[m
[32m+[m[32m  error: "",[m
[32m+[m[32m  queryStatus: "idle",[m
[32m+[m[32m  tracks: null,[m
[32m+[m[32m  playlistPage: 0,[m
[32m+[m[32m  pageLimitOverload: false,[m
[32m+[m[32m  currentPlaylistPage: "1",[m
[32m+[m[32m  formData: {},[m
[32m+[m[32m  creationStatus: false,[m
[32m+[m[32m  preCreateError: "",[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mconst mainReducer = ([m
[32m+[m[32m  state: MainState = initialState,[m
[32m+[m[32m  action: MainAction[m
[32m+[m[32m): MainState => {[m
[32m+[m[32m  switch (action.type) {[m
[32m+[m[32m    case getType(actions.getPlaylistsReq):[m
[32m+[m[32m      return {[m
[32m+[m[32m        ...state,[m
[32m+[m[32m        queryStatus: "playlists_pending",[m
[32m+[m[32m      }[m
[32m+[m[32m    case getType(actions.getTracksReq):[m
[32m+[m[32m      return {[m
[32m+[m[32m        ...state,[m
[32m+[m[32m        queryStatus: "tracks_pending",[m
[32m+[m[32m      }[m
[32m+[m[32m    case getType(actions.getPlaylistsOk):[m
[32m+[m[32m      return {[m
[32m+[m[32m        ...state,[m
[32m+[m[32m        error: "",[m
[32m+[m[32m        playlists: action.payload,[m
[32m+[m[32m        queryStatus: "playlists_succeed",[m
[32m+[m[32m      }[m
[32m+[m[32m    case getType(actions.getTracksOk):[m
[32m+[m[32m      return {[m
[32m+[m[32m        ...state,[m
[32m+[m[32m        error: "",[m
[32m+[m[32m        queryStatus: "tracks_succeed",[m
[32m+[m[32m        tracks: action.payload,[m
[32m+[m[32m      }[m
[32m+[m[32m    case getType(actions.getPlaylistsTracksFail):[m
[32m+[m[32m      return {[m
[32m+[m[32m        ...state,[m
[32m+[m[32m        error: action.payload,[m
[32m+[m[32m        queryStatus: "failure",[m
[32m+[m[32m      }[m
[32m+[m[32m    case getType(actions.prevPlaylistPage):[m
[32m+[m[32m      return {[m
[32m+[m[32m        ...state,[m
[32m+[m[32m        currentPlaylistPage: action.payload,[m
[32m+[m[32m      }[m
[32m+[m[32m    case getType(actions.nextPlaylistPage):[m
[32m+[m[32m      return {[m
[32m+[m[32m        ...state,[m
[32m+[m[32m        currentPlaylistPage: action.payload,[m
[32m+[m[32m      }[m
[32m+[m[32m    case getType(actions.setLimitOverloaded):[m
[32m+[m[32m      return {[m
[32m+[m[32m        ...state,[m
[32m+[m[32m        pageLimitOverload: action.payload,[m
[32m+[m[32m      }[m
[32m+[m[32m      case getType(actions.createPlaylistSuccess):[m
[32m+[m[32m      return {[m
[32m+[m[32m        ...state,[m
[32m+[m[32m        creationStatus: true,[m
[32m+[m[32m        preCreateError: "",[m
[32m+[m[32m      }[m
[32m+[m[32m      case getType(actions.createPlaylistFailure):[m
[32m+[m[32m      return {[m
[32m+[m[32m        ...state,[m
[32m+[m[32m        creationStatus: false,[m
[32m+[m[32m        preCreateError: action.payload,[m
[32m+[m[32m      }[m
[32m+[m[32m    case getType(actions.createPlaylistReq):[m
[32m+[m[32m      return {[m
[32m+[m[32m        ...state,[m
[32m+[m[32m        formData: action.payload,[m
[32m+[m[32m      }[m
[32m+[m[32m    default:[m
[32m+[m[32m      return state[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mexport default mainReducer[m
[1mdiff --git a/src/store/main/saga.ts b/src/store/main/saga.ts[m
[1mnew file mode 100644[m
[1mindex 0000000..1a517a8[m
[1m--- /dev/null[m
[1m+++ b/src/store/main/saga.ts[m
[36m@@ -0,0 +1,384 @@[m
[32m+[m[32mimport { SagaIterator } from "redux-saga"[m
[32m+[m[32mimport { call, put, take } from "redux-saga/effects"[m
[32m+[m[32mimport * as actions from "../actions"[m
[32m+[m[32mimport { myFetch } from "../apiClient"[m
[32m+[m[32mimport { ENDPOINT_UPLOAD } from "./../../store/apiClient"[m
[32m+[m
[32m+[m[32mconst getToken = () => localStorage.getItem("token")[m
[32m+[m[32mconst getPlaylistId = () => localStorage.getItem("playlistId")[m
[32m+[m[32mconst getTracksTrueStatus = () => localStorage.getItem("setTracksTrue")[m
[32m+[m[32mconst getPlaylistPageLength = () => localStorage.getItem("playlistPageLength")[m
[32m+[m[32mconst checkLimitOverLoad = () => localStorage.getItem("limitOverloaded")[m
[32m+[m
[32m+[m[32mconst allTracks = async (acceptedFiles: Array<any>) => {[m
[32m+[m[32m  return await Promise.all([m
[32m+[m[32m    acceptedFiles.map(async (file: string | Blob) => {[m
[32m+[m[32m      const body = new FormData()[m
[32m+[m[32m      body.append("track", file)[m
[32m+[m[32m      let response = await fetch(`${ENDPOINT_UPLOAD}`, {[m
[32m+[m[32m        method: "POST",[m
[32m+[m[32m        headers: localStorage.token[m
[32m+[m[32m          ? { Authorization: "Bearer " + localStorage.token }[m
[32m+[m[32m          : {},[m
[32m+[m[32m        body: body,[m
[32m+[m[32m      })[m
[32m+[m[32m      let res = await response.json()[m
[32m+[m[32m      return res[m
[32m+[m[32m    })[m
[32m+[m[32m  )[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mconst infoPreparingToServer = (arr: Array<any>) => {[m
[32m+[m[32m  for (let obj of arr) {[m
[32m+[m[32m    for (let key in obj) {[m
[32m+[m[32m      if (key === "url") {[m
[32m+[m[32m        delete obj[key][m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m  return arr[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mconst PAGE_LIMIT = 20[m
[32m+[m
[32m+[m[32mconst queryPlaylists = `[m
[32m+[m[32mquery findPlaylists($query: String!){[m
[32m+[m[32m    PlaylistFind(query:$query){[m
[32m+[m[32m    name[m
[32m+[m[32m    _id[m
[32m+[m[32m    tracks{[m
[32m+[m[32m      _id[m
[32m+[m[32m      originalFileName[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m[32m`[m
[32m+[m[32mconst testQueryPlaylists = `[m
[32m+[m[32mquery findPlaylists{[m
[32m+[m[32m    PlaylistFind(query:"[{}]"){[m
[32m+[m[32m    name[m
[32m+[m[32m    _id[m
[32m+[m[32m    tracks{[m
[32m+[m[32m      _id[m
[32m+[m[32m      originalFileName[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m[32m`[m
[32m+[m
[32m+[m[32mconst queryTracks = `[m
[32m+[m[32mquery allTracks($query: String!){[m
[32m+[m[32m    TrackFind(query: $query){[m
[32m+[m[32m      url[m
[32m+[m[32m      _id[m
[32m+[m[32m      originalFileName[m
[32m+[m[32m      id3{[m
[32m+[m[32m        title[m
[32m+[m[32m        album[m
[32m+[m[32m        artist[m
[32m+[m[32m        year[m
[32m+[m[32m        genre[m
[32m+[m[32m        trackNumber[m
[32m+[m[32m      }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m[32m`[m
[32m+[m
[32m+[m[32mconst queryPlaylistOne = `[m
[32m+[m[32mquery playlistOne($query: String!){[m
[32m+[m[32m  PlaylistFindOne(query: $query){[m
[32m+[m[32m    _id[m
[32m+[m[32m    name[m
[32m+[m[32m    tracks{[m
[32m+[m[32m      _id[m
[32m+[m[32m      originalFileName[m
[32m+[m[32m      url[m
[32m+[m[32m      id3{[m
[32m+[m[32m        title[m
[32m+[m[32m        album[m
[32m+[m[32m        artist[m
[32m+[m[32m        year[m
[32m+[m[32m        genre[m
[32m+[m[32m        trackNumber[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m[32m`[m
[32m+[m
[32m+[m[32mconst createPlaylist = `[m
[32m+[m[32mmutation addPlaylist($playlist: PlaylistInput!){[m
[32m+[m[32m  PlaylistUpsert(playlist: $playlist){[m
[32m+[m[32m    name[m
[32m+[m[32m    description[m
[32m+[m[32m    tracks{[m
[32m+[m[32m      _id[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m[32m`[m
[32m+[m
[32m+[m[32mexport function* getPlaylistsSaga(): SagaIterator {[m
[32m+[m[32m  while (true) {[m
[32m+[m[32m    console.log("start getPlaylistsSaga")[m
[32m+[m[32m    try {[m
[32m+[m[32m      yield take(actions.authSuccess)[m
[32m+[m[32m      const authData = yield call(getToken)[m
[32m+[m[32m      yield put(actions.getPlaylistsReq())[m
[32m+[m[32m      console.log("getPlaylistsReq()")[m
[32m+[m[32m      const allPlaylists = yield call([m
[32m+[m[32m        myFetch,[m
[32m+[m[32m        testQueryPlaylists,[m
[32m+[m[32m        {},[m
[32m+[m[32m        { headers: { Authorization: `Bearer ${authData}` } }[m
[32m+[m[32m      )[m
[32m+[m
[32m+[m[32m      yield put(actions.setPlaylistPageLength(allPlaylists.PlaylistFind.length))[m
[32m+[m[32m      localStorage.setItem([m
[32m+[m[32m        "playlistPageLength",[m
[32m+[m[32m        `${allPlaylists.PlaylistFind.length}`[m
[32m+[m[32m      )[m
[32m+[m[32m      yield put(actions.setPageLimit(PAGE_LIMIT))[m
[32m+[m[32m      const limitedPlaylists = yield call([m
[32m+[m[32m        myFetch,[m
[32m+[m[32m        queryPlaylists,[m
[32m+[m[32m        {[m
[32m+[m[32m          query: JSON.stringify([[m
[32m+[m[32m            {},[m
[32m+[m[32m            {[m
[32m+[m[32m              // sort: [{ _id: -1 }],[m
[32m+[m[32m              // skip: [10],[m
[32m+[m[32m              limit: [PAGE_LIMIT],[m
[32m+[m[32m            },[m
[32m+[m[32m          ]),[m
[32m+[m[32m        },[m
[32m+[m[32m        { headers: { Authorization: `Bearer ${authData}` } }[m
[32m+[m[32m      )[m
[32m+[m[32m      yield put(actions.getPlaylistsOk(limitedPlaylists))[m
[32m+[m[32m    } catch (e) {[m
[32m+[m[32m      yield put(actions.getPlaylistsTracksFail(e))[m
[32m+[m[32m      throw new Error(`ВСЕ ПРОПАЛО! ${e}`)[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mexport function* getTracksSaga(): SagaIterator {[m
[32m+[m[32m  while (true) {[m
[32m+[m[32m    console.log("start getTracksSaga")[m
[32m+[m[32m    const playlistIdOld = yield call(getPlaylistId)[m
[32m+[m[32m    const tracksTrueStatus = yield call(getTracksTrueStatus)[m
[32m+[m
[32m+[m[32m    try {[m
[32m+[m[32m      if (tracksTrueStatus === "ok") {[m
[32m+[m[32m        console.log("tracksTrueStatus ОК")[m
[32m+[m[32m        const authData = yield call(getToken)[m
[32m+[m[32m        const { payload: playlistId } = yield take(actions.getTracksReq)[m
[32m+[m[32m        localStorage.setItem("playlistId", playlistId)[m
[32m+[m[32m        yield put(actions.getTracksReq(""))[m
[32m+[m[32m        const searchQuery = `${playlistId}`[m
[32m+[m[32m        const playlistOne = yield call([m
[32m+[m[32m          myFetch,[m
[32m+[m[32m          queryPlaylistOne,[m
[32m+[m[32m          {[m
[32m+[m[32m            query: JSON.stringify([[m
[32m+[m[32m              {[m
[32m+[m[32m                _id: searchQuery,[m
[32m+[m[32m              },[m
[32m+[m[32m            ]),[m
[32m+[m[32m          },[m
[32m+[m[32m          { headers: { Authorization: `Bearer ${authData}` } }[m
[32m+[m[32m        )[m
[32m+[m[32m        yield put(actions.getTracksOk(playlistOne))[m
[32m+[m[32m        continue[m
[32m+[m[32m      }[m
[32m+[m[32m      if (playlistIdOld) {[m
[32m+[m[32m        console.log("начат блок кода, когда есть playlistIdOld")[m
[32m+[m[32m        yield take(actions.getPlaylistsOk)[m
[32m+[m[32m        yield put(actions.getTracksReq(""))[m
[32m+[m[32m        const searchQueryOld = `${playlistIdOld}`[m
[32m+[m[32m        const authData = yield call(getToken)[m
[32m+[m[32m        const playlistOneOld = yield call([m
[32m+[m[32m          myFetch,[m
[32m+[m[32m          queryPlaylistOne,[m
[32m+[m[32m          {[m
[32m+[m[32m            query: JSON.stringify([[m
[32m+[m[32m              {[m
[32m+[m[32m                _id: searchQueryOld,[m
[32m+[m[32m              },[m
[32m+[m[32m            ]),[m
[32m+[m[32m          },[m
[32m+[m[32m          { headers: { Authorization: `Bearer ${authData}` } }[m
[32m+[m[32m        )[m
[32m+[m[32m        yield put(actions.getTracksOk(playlistOneOld))[m
[32m+[m[32m        localStorage.setItem("setTracksTrue", "ok")[m
[32m+[m[32m      }[m
[32m+[m[32m      console.log("начальная загрузка треков после логина окончена")[m
[32m+[m
[32m+[m[32m      // начальная загрузка треков после логина[m
[32m+[m[32m      yield take(actions.getPlaylistsOk)[m
[32m+[m[32m      const authData = yield call(getToken)[m
[32m+[m[32m      yield put(actions.getTracksReq(""))[m
[32m+[m[32m      const tracks = yield call([m
[32m+[m[32m        myFetch,[m
[32m+[m[32m        queryTracks,[m
[32m+[m[32m        {[m
[32m+[m[32m          query: JSON.stringify([[m
[32m+[m[32m            {},[m
[32m+[m[32m            {[m
[32m+[m[32m              limit: [PAGE_LIMIT],[m
[32m+[m[32m            },[m
[32m+[m[32m          ]),[m
[32m+[m[32m        },[m
[32m+[m[32m        { headers: { Authorization: `Bearer ${authData}` } }[m
[32m+[m[32m      )[m
[32m+[m[32m      console.log("начат блок кода, когда нету playlistIdOld")[m
[32m+[m[32m      yield put(actions.getTracksOk(tracks))[m
[32m+[m
[32m+[m[32m      // загрузка треков по запросу[m
[32m+[m[32m      console.log("ожидание загрузки треков по запросу")[m
[32m+[m[32m      const { payload: playlistId } = yield take(actions.getTracksReq)[m
[32m+[m[32m      localStorage.setItem("playlistId", playlistId)[m
[32m+[m[32m      yield put(actions.getTracksReq(""))[m
[32m+[m[32m      const searchQuery = `${playlistId}`[m
[32m+[m[32m      const playlistOne = yield call([m
[32m+[m[32m        myFetch,[m
[32m+[m[32m        queryPlaylistOne,[m
[32m+[m[32m        {[m
[32m+[m[32m          query: JSON.stringify([[m
[32m+[m[32m            {[m
[32m+[m[32m              _id: searchQuery,[m
[32m+[m[32m            },[m
[32m+[m[32m          ]),[m
[32m+[m[32m        },[m
[32m+[m[32m        { headers: { Authorization: `Bearer ${authData}` } }[m
[32m+[m[32m      )[m
[32m+[m[32m      yield put(actions.getTracksOk(playlistOne))[m
[32m+[m[32m      localStorage.setItem("setTracksTrue", "ok")[m
[32m+[m[32m    } catch (e) {[m
[32m+[m[32m      yield put(actions.getPlaylistsTracksFail(e))[m
[32m+[m[32m      throw new Error(`ВСЕ ПРОПАЛО! ${e}`)[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mlet i = 0[m
[32m+[m[32mlet j = 0[m
[32m+[m[32mlet pageNum = 1[m
[32m+[m[32mexport function* getPrevPlaylistPageSaga(): SagaIterator {[m
[32m+[m[32m  while (true) {[m
[32m+[m[32m    let prevPlaylistQuery = 0[m
[32m+[m[32m    yield take(actions.prevPlaylistPage)[m
[32m+[m[32m    i = i - PAGE_LIMIT[m
[32m+[m[32m    const authData = yield call(getToken)[m
[32m+[m[32m    prevPlaylistQuery = i[m
[32m+[m[32m    yield put(actions.getPlaylistsReq())[m
[32m+[m[32m    console.log("getPlaylistsReq() prev")[m
[32m+[m[32m    try {[m
[32m+[m[32m      const limitedPlaylists = yield call([m
[32m+[m[32m        myFetch,[m
[32m+[m[32m        queryPlaylists,[m
[32m+[m[32m        {[m
[32m+[m[32m          query: JSON.stringify([[m
[32m+[m[32m            {},[m
[32m+[m[32m            {[m
[32m+[m[32m              // sort: [{ _id: -1 }],[m
[32m+[m[32m              skip: [prevPlaylistQuery],[m
[32m+[m[32m              limit: [PAGE_LIMIT],[m
[32m+[m[32m            },[m
[32m+[m[32m          ]),[m
[32m+[m[32m        },[m
[32m+[m[32m        { headers: { Authorization: `Bearer ${authData}` } }[m
[32m+[m[32m      )[m
[32m+[m
[32m+[m[32m      yield put(actions.getPlaylistsOk(limitedPlaylists))[m
[32m+[m[32m      pageNum = pageNum - 1[m
[32m+[m[32m      yield put(actions.prevPlaylistPage(`${pageNum}`))[m
[32m+[m[32m      j = j - PAGE_LIMIT - 1[m
[32m+[m[32m      yield put(actions.setSkippedPlaylistsData(j))[m
[32m+[m[32m      if (j < PAGE_LIMIT - 1) {[m
[32m+[m[32m        yield put(actions.prevPlaylistPage("1"))[m
[32m+[m[32m      }[m
[32m+[m[32m      const checkTrue = yield call(checkLimitOverLoad)[m
[32m+[m[32m      if (checkTrue) {[m
[32m+[m[32m        yield put(actions.setLimitOverloaded(false))[m
[32m+[m[32m        localStorage.removeItem("limitOverloaded")[m
[32m+[m[32m      }[m
[32m+[m[32m    } catch (e) {[m
[32m+[m[32m      yield put(actions.getPlaylistsTracksFail(e))[m
[32m+[m[32m      throw new Error(`ВСЕ ПРОПАЛО! ${e}`)[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mexport function* getNextPlaylistPageSaga(): SagaIterator {[m
[32m+[m[32m  while (true) {[m
[32m+[m[32m    let nextPlaylistQuery = 0[m
[32m+[m[32m    yield take(actions.nextPlaylistPage)[m
[32m+[m[32m    i = i + PAGE_LIMIT[m
[32m+[m[32m    const authData = yield call(getToken)[m
[32m+[m[32m    nextPlaylistQuery = i[m
[32m+[m
[32m+[m[32m    yield put(actions.getPlaylistsReq())[m
[32m+[m[32m    console.log("getPlaylistsReq() next")[m
[32m+[m[32m    try {[m
[32m+[m[32m      const limitedPlaylists = yield call([m
[32m+[m[32m        myFetch,[m
[32m+[m[32m        queryPlaylists,[m
[32m+[m[32m        {[m
[32m+[m[32m          query: JSON.stringify([[m
[32m+[m[32m            {},[m
[32m+[m[32m            {[m
[32m+[m[32m              // sort: [{ _id: -1 }],[m
[32m+[m[32m              skip: [nextPlaylistQuery],[m
[32m+[m[32m              limit: [PAGE_LIMIT],[m
[32m+[m[32m            },[m
[32m+[m[32m          ]),[m
[32m+[m[32m        },[m
[32m+[m[32m        { headers: { Authorization: `Bearer ${authData}` } }[m
[32m+[m[32m      )[m
[32m+[m
[32m+[m[32m      yield put(actions.getPlaylistsOk(limitedPlaylists))[m
[32m+[m[32m      pageNum = pageNum + 1[m
[32m+[m[32m      yield put(actions.nextPlaylistPage(`${pageNum}`))[m
[32m+[m[32m      j = j + PAGE_LIMIT - 1[m
[32m+[m[32m      yield put(actions.setSkippedPlaylistsData(j))[m
[32m+[m[32m      const playlistLength = yield call(getPlaylistPageLength)[m
[32m+[m[32m      if (pageNum * (PAGE_LIMIT - 1) > playlistLength) {[m
[32m+[m[32m        yield put(actions.setLimitOverloaded(true))[m
[32m+[m[32m        j = +playlistLength[m
[32m+[m[32m        localStorage.setItem("limitOverloaded", "yes")[m
[32m+[m[32m      }[m
[32m+[m[32m    } catch (e) {[m
[32m+[m[32m      yield put(actions.getPlaylistsTracksFail(e))[m
[32m+[m[32m      throw new Error(`ВСЕ ПРОПАЛО! ${e}`)[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mexport function* createPlaylistSaga(): SagaIterator {[m
[32m+[m[32m  while (true) {[m
[32m+[m[32m    try {[m
[32m+[m[32m      const { payload: data } = yield take(actions.createPlaylistReq)[m
[32m+[m[32m      const tracksServerInfo = yield call(allTracks, data.files)[m
[32m+[m[32m      const res = yield call(infoPreparingToServer, tracksServerInfo)[m
[32m+[m[32m      const authData = yield call(getToken)[m
[32m+[m[32m      yield call([m
[32m+[m[32m        myFetch,[m
[32m+[m[32m        createPlaylist,[m
[32m+[m[32m        {[m
[32m+[m[32m          playlist: {[m
[32m+[m[32m            name: data.name,[m
[32m+[m[32m            description: data.description,[m
[32m+[m[32m            tracks: res,[m
[32m+[m[32m          },[m
[32m+[m[32m        },[m
[32m+[m[32m        { headers: { Authorization: `Bearer ${authData}` } }[m
[32m+[m[32m      )[m
[32m+[m[32m      yield put(actions.createPlaylistSuccess())[m
[32m+[m[32m      yield put(actions.getPlaylistsReq())[m
[32m+[m[32m    } catch (e) {[m
[32m+[m[32m      yield put(actions.createPlaylistFailure(e))[m
[32m+[m[32m      throw new Error(`Playlist creation rejected: ${e}`)[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m

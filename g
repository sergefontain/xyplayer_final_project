[32m+[m[32mimport React from "react"[m
[32m+[m
[32m+[m[32minterface classProps {[m
[32m+[m[32m  url: string[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32minterface AudioReturn {[m
[32m+[m[32m  play: boolean[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mvar i = 0[m
[32m+[m[32mlet ctx, x_end, y_end, bar_height[m
[32m+[m[32mconst bars = 50[m
[32m+[m[32mconst radius = 0[m
[32m+[m[32mconst bar_width = 20[m
[32m+[m[32mconst browserInterfaceDiffWidth = 30[m
[32m+[m[32mconst browserInterfaceDiffHeight = 100[m
[32m+[m
[32m+[m[32mexport class Music extends React.PureComponent<classProps, AudioReturn> {[m
[32m+[m[32m  audio: globalThis.HTMLAudioElement[m
[32m+[m[32m  source: any[m
[32m+[m[32m  analyser: any[m
[32m+[m[32m  frequency_array: any[m
[32m+[m[32m  canvas: React.RefObject<HTMLCanvasElement>[m
[32m+[m[32m  rafId!: number[m
[32m+[m
[32m+[m[32m  constructor(props: classProps) {[m
[32m+[m[32m    super(props)[m
[32m+[m[32m    this.state = {[m
[32m+[m[32m      play: false,[m
[32m+[m[32m    }[m
[32m+[m[32m    this.audio = new Audio(this.props.url)[m
[32m+[m[32m    this.canvas = React.createRef()[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  componentDidMount() {[m
[32m+[m[32m    this.audio.addEventListener("ended", () => this.setState({ play: false }))[m
[32m+[m
[32m+[m[32m    this.context = new AudioContext()[m
[32m+[m[32m    this.source = this.context.createMediaElementSource(this.audio)[m
[32m+[m[32m    this.analyser = this.context.createAnalyser()[m
[32m+[m[32m    this.source.connect(this.analyser)[m
[32m+[m[32m    this.analyser.connect(this.context.destination)[m
[32m+[m[32m    this.frequency_array = new Uint8Array(this.analyser.frequencyBinCount)[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  componentWillUnmount() {[m
[32m+[m[32m    this.audio.removeEventListener("ended", () =>[m
[32m+[m[32m      this.setState({ play: false })[m
[32m+[m[32m    )[m
[32m+[m[32m    cancelAnimationFrame(this.rafId)[m
[32m+[m[32m    this.analyser.disconnect()[m
[32m+[m[32m    this.source.disconnect()[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  animationLooper(canvas: any) {[m
[32m+[m[32m    let width = (canvas.width = window.innerWidth - browserInterfaceDiffWidth)[m
[32m+[m[32m    let height = (canvas.height = window.innerHeight - browserInterfaceDiffHeight)[m
[32m+[m[32m    let center_x = width / 2 + browserInterfaceDiffWidth/2[m
[32m+[m[32m    let center_y = height / 2 + browserInterfaceDiffHeight/2[m
[32m+[m[32m    ctx = canvas.getContext("2d")[m
[32m+[m
[32m+[m[32m    for (var i = 0; i < bars; i++) {[m
[32m+[m[32m      //divide a circle into equal part[m
[32m+[m[32m      const rads = (Math.PI * 2) / bars[m
[32m+[m
[32m+[m[32m      // Math is magical[m
[32m+[m[32m      // bar_height = this.frequency_array[i] * 3[m
[32m+[m[32m      bar_height = this.frequency_array[i] * 1.33[m
[32m+[m[32m      let x = center_x + Math.cos(rads * i) * (radius + bar_height / 2)[m
[32m+[m[32m      let y = center_y + Math.sin(rads * i) * (radius + bar_height / 2)[m
[32m+[m[32m      x_end = center_x + Math.cos(rads * i) * (radius + bar_height)[m
[32m+[m[32m      y_end = center_y + Math.sin(rads * i) * (radius + bar_height)[m
[32m+[m
[32m+[m[32m      //draw a bar[m
[32m+[m[32m      this.drawBar(x, y, x_end, y_end, this.frequency_array[i], ctx, canvas)[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  drawBar([m
[32m+[m[32m    x1 = 0,[m
[32m+[m[32m    y1 = 0,[m
[32m+[m[32m    x2 = 0,[m
[32m+[m[32m    y2 = 0,[m
[32m+[m[32m    frequency: number,[m
[32m+[m[32m    ctx: {[m
[32m+[m[32m      createLinearGradient: ([m
[32m+[m[32m        arg0: number,[m
[32m+[m[32m        arg1: number,[m
[32m+[m[32m        arg2: number,[m
[32m+[m[32m        arg3: number,[m
[32m+[m[32m      ) => any[m
[32m+[m[32m      fillStyle: any[m
[32m+[m[32m      strokeStyle: string | undefined[m
[32m+[m[32m      lineWidth: number[m
[32m+[m[32m      beginPath: () => void[m
[32m+[m[32m      moveTo: (arg0: number, arg1: number) => void[m
[32m+[m[32m      lineTo: (arg0: number, arg1: number) => void[m
[32m+[m[32m      stroke: () => void[m
[32m+[m[32m      closePath: () => void[m
[32m+[m[32m      clearRect: ([m
[32m+[m[32m        arg0: number,[m
[32m+[m[32m        arg1: number,[m
[32m+[m[32m        arg2: number,[m
[32m+[m[32m        arg3: number[m
[32m+[m[32m      ) => void[m
[32m+[m[32m    },[m
[32m+[m[32m    canvas: { width: number; height: number }[m
[32m+[m[32m  ) {[m
[32m+[m[32m    const gradient = ctx.createLinearGradient(0, 0, canvas.width, canvas.height)[m
[32m+[m[32m    gradient.addColorStop(0, "white")[m
[32m+[m[32m    gradient.addColorStop(1, "green")[m
[32m+[m[32m    ctx.fillStyle = gradient[m
[32m+[m
[32m+[m[32m    let lineColor[m
[32m+[m[32m    if (frequency < 75) {[m
[32m+[m[32m      if (this.state.play) {[m
[32m+[m[32m        i = i + 1[m
[32m+[m[32m        if (i / 10 < 40) {[m
[32m+[m[32m          lineColor = `rgb(${238},${10},${230}` // ярко-фиолетовый[m
[32m+[m[32m        }[m
[32m+[m[32m        if (i / 10 >= 40 && i / 10 < 120) {[m
[32m+[m[32m          lineColor = `rgb(${0},${120},${250}` // голубой[m
[32m+[m[32m        }[m
[32m+[m[32m        if (i / 10 >= 120 && i / 10 < 260) {[m
[32m+[m[32m          lineColor = `rgb(${34},${217},${0}` // зеленый[m
[32m+[m[32m        }[m
[32m+[m[32m        if (i / 10 >= 260 && i / 10 < 380) {[m
[32m+[m[32m          lineColor = `rgb(${255},${138},${0}` // оранженый[m
[32m+[m[32m        }[m
[32m+[m[32m        if (i / 10 > 380) {[m
[32m+[m[32m          lineColor = `rgb(${255},${0},${0}` // насыщенный красный[m
[32m+[m[32m        }[m
[32m+[m[32m        if (i / 10 === 500) {[m
[32m+[m[32m          i = 0[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    } else if (frequency >= 75 && frequency < 120) {[m
[32m+[m[32m      if (i / 10 >= 120 && i / 10 < 260) {[m
[32m+[m[32m        lineColor = `rgb(${184},${250},${220}` // бирюзовый (светло-зеленый+светло-голубой)[m
[32m+[m[32m      } else {[m
[32m+[m[32m        lineColor = `rgb(${184 * Math.random()},${250},${220}`[m
[32m+[m[32m      }[m
[32m+[m[32m    } else if (frequency >= 120 && frequency < 160) {[m
[32m+[m[32m      if (i / 10 >= 260 && i / 10 < 380) {[m
[32m+[m[32m        lineColor = `rgb(${229},${209},${228})` // светло-фиолетовый[m
[32m+[m[32m      } else {[m
[32m+[m[32m        lineColor = `rgb(${10 * frequency * Math.random()},${[m
[32m+[m[32m          2 * frequency * Math.random()[m
[32m+[m[32m        },${5 * frequency * Math.random()})`[m
[32m+[m[32m      }[m
[32m+[m[32m    } else {[m
[32m+[m[32m      lineColor = `rgb(${238},${230},${10})` // желтый[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    ctx.strokeStyle = lineColor[m
[32m+[m[32m    ctx.lineWidth = bar_width[m
[32m+[m[32m    ctx.beginPath()[m
[32m+[m[32m    ctx.moveTo(x1, y1)[m
[32m+[m[32m    ctx.lineTo(x2, y2)[m
[32m+[m[32m    ctx.stroke()[m
[32m+[m[32m    ctx.closePath()[m
[32m+[m
[32m+[m[32m    if (!this.state.play) {[m
[32m+[m[32m      ctx.clearRect(0, 0, canvas.width, canvas.height)[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  tick = () => {[m
[32m+[m[32m    this.animationLooper(this.canvas.current)[m
[32m+[m[32m    this.analyser.getByteTimeDomainData(this.frequency_array)[m
[32m+[m[32m    this.rafId = requestAnimationFrame(this.tick)[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  togglePlay = () => {[m
[32m+[m[32m    this.setState({ play: !this.state.play }, () => {[m
[32m+[m[32m      if (this.state.play) {[m
[32m+[m[32m        this.audio.play()[m
[32m+[m[32m        this.rafId = requestAnimationFrame(this.tick)[m
[32m+[m[32m      } else {[m
[32m+[m[32m        this.audio.pause()[m
[32m+[m[32m        cancelAnimationFrame(this.rafId)[m
[32m+[m[32m      }[m
[32m+[m[32m    })[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  render() {[m
[32m+[m[32m    return ([m
[32m+[m[32m      <>[m
[32m+[m[32m        <button onClick={this.togglePlay}>[m
[32m+[m[32m          {this.state.play ? "Pause" : "Play"}[m
[32m+[m[32m        </button>[m
[32m+[m[32m        <canvas ref={this.canvas} />[m
[32m+[m[32m      </>[m
[32m+[m[32m    )[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mexport default Music[m

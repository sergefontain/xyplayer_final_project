[32m+[m[32mimport React, { useRef } from "react"[m
[32m+[m[32mimport { connect } from "react-redux"[m
[32m+[m[32mimport { bindActionCreators, Dispatch } from "redux"[m
[32m+[m[32mimport { RootAction, RootState } from "../../store/rootReducer"[m
[32m+[m[32mimport * as playerActions from "../../store/actions"[m
[32m+[m[32mimport Music from "../AudioPlayer"[m
[32m+[m[32mimport song01 from "./01.mp3"[m
[32m+[m[32mimport song02 from "./02.mp3"[m
[32m+[m[32mimport song03 from "./03.mp3"[m
[32m+[m
[32m+[m[32mtype Props = ReturnType<typeof mapStateToProps> &[m
[32m+[m[32m  ReturnType<typeof mapDispatchToProps>[m
[32m+[m
[32m+[m[32mconst mapStateToProps = (state: RootState) => ({[m
[32m+[m[32m  oldVolValueToProps: state.play.volValue,[m
[32m+[m[32m  oldCurrTimeToProps: state.play.currTime,[m
[32m+[m[32m})[m
[32m+[m[32mconst mapDispatchToProps = (dispatch: Dispatch<RootAction>) =>[m
[32m+[m[32m  bindActionCreators([m
[32m+[m[32m    {[m
[32m+[m[32m      setOldVolValue: (payload) => playerActions.savingVolValue(payload),[m
[32m+[m[32m      setOldCurrTime: (payload) => playerActions.savingCurrTime(payload),[m
[32m+[m[32m    },[m
[32m+[m[32m    dispatch[m
[32m+[m[32m  )[m
[32m+[m
[32m+[m[32mconst GetStarted: React.FC<Props> = ({[m
[32m+[m[32m  setOldVolValue,[m
[32m+[m[32m  oldVolValueToProps,[m
[32m+[m[32m  setOldCurrTime,[m
[32m+[m[32m  oldCurrTimeToProps,[m
[32m+[m[32m}) => {[m
[32m+[m[32m  const [timeInputValueCurrent, setTimeInputValueCurrent] = React.useState("0")[m
[32m+[m[32m  const [totalTimeDuration, setTotalTimeDuration] = React.useState(0)[m
[32m+[m
[32m+[m[32m  const [totalMinDuration, setTotalMinDuration] = React.useState(0)[m
[32m+[m[32m  const [secBalanceDuration, setSecBalanceDuration] = React.useState(0)[m
[32m+[m[32m  const [currMinTime, setCurrMinTime] = React.useState("00")[m
[32m+[m[32m  const [currSecBalanceTime, setCurrSecBalanceTime] = React.useState("00")[m
[32m+[m
[32m+[m[32m  const [volInputValue, setVolInputValue] = React.useState(0)[m
[32m+[m[32m  const [volTrack, setVolTrack] = React.useState(0)[m
[32m+[m[32m  const [volLoopValue, setLoopValue] = React.useState(false)[m
[32m+[m[32m  const [volMute, setVolMute] = React.useState(false)[m
[32m+[m
[32m+[m[32m  const ref: any = useRef()[m
[32m+[m[32m  const refVolRange: any = useRef()[m
[32m+[m[32m  const refTimeRange: any = useRef()[m
[32m+[m
[32m+[m[32m  React.useEffect(() => {[m
[32m+[m[32m    const audioTrack1 = ref.current[m
[32m+[m[32m    const volRanger = refVolRange.current[m
[32m+[m[32m    const timeRanger = refTimeRange.current[m
[32m+[m
[32m+[m[32m    audioTrack1.loop = volLoopValue[m
[32m+[m[32m    audioTrack1.volume = volTrack[m
[32m+[m[32m    audioTrack1.currentTime = timeInputValueCurrent[m
[32m+[m[32m//  console.log("+", oldCurrTimeToProps)[m
[32m+[m
[32m+[m[32m    audioTrack1.ontimeupdate = (e: any) => {[m
[32m+[m[32m      timeRanger.min = "0"[m
[32m+[m[32m      timeRanger.max = `${totalTimeDuration}`[m
[32m+[m[32m      timeRanger.value = e.target.currentTime.toFixed(0)[m
[32m+[m
[32m+[m[32m      // setOldCurrTime(e.target.currentTime.toFixed(0))[m
[32m+[m[32m      // console.log("1", timeInputValueCurrent)[m
[32m+[m[32m      // console.log("2", e.target.currentTime.toFixed(0))[m
[32m+[m
[32m+[m[32m      ;(function timeDivider(currSec: string, counter: string = "00"): any {[m
[32m+[m[32m        let newCounterValue = 60 * (+counter + 1)[m
[32m+[m[32m        if (+currSec < newCounterValue) {[m
[32m+[m[32m          if (+currSec < 60) {[m
[32m+[m[32m            setCurrMinTime(`${+counter}`.padStart(2, counter))[m
[32m+[m[32m            setCurrSecBalanceTime(`${+currSec}`.padStart(2, "00"))[m
[32m+[m[32m          } else if (+currSec === 60) {[m
[32m+[m[32m            setCurrSecBalanceTime("00")[m
[32m+[m[32m          } else {[m
[32m+[m[32m            setCurrSecBalanceTime(`${+currSec - 60 * +counter}`.padStart(2, "00"))[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m[32m        if (+currSec >= +totalTimeDuration - 1) {[m
[32m+[m[32m          setCurrMinTime(`${Math.trunc(+currSec / 60)}`.padStart(2, "00"))[m
[32m+[m[32m          return[m
[32m+[m[32m        }[m
[32m+[m[32m        if (+currSec >= newCounterValue - 1 && +currSec !== 0) {[m
[32m+[m[32m          setCurrMinTime(`${+counter + 1}`.padStart(2, "00"))[m
[32m+[m[32m          return timeDivider([m
[32m+[m[32m            `${+currSec + 1}`,[m
[32m+[m[32m            `${+counter + 1}`.padStart(2, counter)[m
[32m+[m[32m          )[m
[32m+[m[32m        }[m
[32m+[m[32m      })(e.target.currentTime.toFixed(0))[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // ;(function timeDivider(currSec: string, counter: string = "00"): any {[m
[32m+[m[32m    //   let newCounterValue = 60 * (+counter + 1)[m
[32m+[m[32m    //   if (+currSec < newCounterValue) {[m
[32m+[m[32m    //     if (+currSec < 60) {[m
[32m+[m[32m    //       setCurrMinTime(`${+counter}`.padStart(2, counter))[m
[32m+[m[32m    //       setCurrSecBalanceTime(`${+currSec}`.padStart(2, "00"))[m
[32m+[m[32m    //     } else if (+currSec === 60) {[m
[32m+[m[32m    //       setCurrSecBalanceTime("00")[m
[32m+[m[32m    //     } else {[m
[32m+[m[32m    //       setCurrSecBalanceTime(`${+currSec - 60 * +counter}`.padStart(2, "00"))[m
[32m+[m[32m    //     }[m
[32m+[m[32m    //   }[m
[32m+[m[32m    //   if (+currSec >= +totalTimeDuration - 1) {[m
[32m+[m[32m    //     setCurrMinTime(`${Math.trunc(+currSec / 60)}`.padStart(2, "00"))[m
[32m+[m[32m    //     return[m
[32m+[m[32m    //   }[m
[32m+[m[32m    //   if (+currSec >= newCounterValue - 1 && +currSec !== 0) {[m
[32m+[m[32m    //     setCurrMinTime(`${+counter + 1}`.padStart(2, "00"))[m
[32m+[m[32m    //     return timeDivider([m
[32m+[m[32m    //       `${+currSec + 1}`,[m
[32m+[m[32m    //       `${+counter + 1}`.padStart(2, counter)[m
[32m+[m[32m    //     )[m
[32m+[m[32m    //   }[m
[32m+[m[32m    // })(timeInputValueCurrent)[m
[32m+[m
[32m+[m[32m    // timeRanger.onchange = () => {[m
[32m+[m[32m    //   setOldCurrTime(timeRanger.value)[m
[32m+[m[32m    // }[m
[32m+[m
[32m+[m[32m    // volRanger.onchange = (e: any) => {[m
[32m+[m[32m    //   console.log("3", oldCurrTimeToProps)[m
[32m+[m[32m    //   setOldCurrTime(audioTrack1.currentTime)[m
[32m+[m[32m    //   console.log("4", oldCurrTimeToProps)[m
[32m+[m[32m    // }[m
[32m+[m[32m  }, [[m
[32m+[m[32m    volTrack,[m
[32m+[m[32m    volLoopValue,[m
[32m+[m[32m    totalTimeDuration,[m
[32m+[m[32m    timeInputValueCurrent,[m
[32m+[m[32m    oldCurrTimeToProps,[m
[32m+[m[32m    setOldCurrTime,[m
[32m+[m[32m  ])[m
[32m+[m
[32m+[m[32m  const volValueHandler = (e: any) => {[m
[32m+[m[32m    let newVal = (e.target.value / 100).toFixed(2)[m
[32m+[m[32m    setOldVolValue(newVal)[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  const volValueChanger = (e: any) => {[m
[32m+[m[32m    let newVal = +(e.target.value / 100).toFixed(2)[m
[32m+[m[32m    setVolTrack(newVal)[m
[32m+[m[32m    setVolInputValue(e.target.value)[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  const timeValueChanger = (e: any) => {[m
[32m+[m[32m    e.target.min = "0"[m
[32m+[m[32m    e.target.max = `${totalTimeDuration}`[m
[32m+[m[32m    setTimeInputValueCurrent(e.target.value)[m
[32m+[m[32m    // setOldCurrTime(e.target.value)[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  const loopInstall = () => {[m
[32m+[m[32m    setLoopValue(!volLoopValue)[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  const muteInstall = () => {[m
[32m+[m[32m    if (!volMute) {[m
[32m+[m[32m      setVolTrack(0)[m
[32m+[m[32m      setVolInputValue(0)[m
[32m+[m[32m    } else {[m
[32m+[m[32m      setVolTrack(oldVolValueToProps)[m
[32m+[m[32m      setVolInputValue(oldVolValueToProps * 100)[m
[32m+[m[32m    }[m
[32m+[m[32m    setVolMute(!volMute)[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  const loadTrackDuration = (e: any) => {[m
[32m+[m[32m    setTotalMinDuration(Math.trunc(e.target.duration / 60))[m
[32m+[m[32m    setSecBalanceDuration([m
[32m+[m[32m      Math.round(e.target.duration - Math.trunc(e.target.duration / 60) * 60)[m
[32m+[m[32m    )[m
[32m+[m[32m    setTotalTimeDuration(e.target.duration)[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[41m  [m
[32m+[m
[32m+[m[32m  return ([m
[32m+[m[32m    <main id="login">[m
[32m+[m[32m      <div className="container-fluid">[m
[32m+[m[32m        <div className="row">[m
[32m+[m[32m          <div className="col" style={{ minWidth: "600px" }}>[m
[32m+[m[32m            <div className="row">[m
[32m+[m[32m              <div className="col">[m
[32m+[m[32m                <audio[m

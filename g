[32m+[m[32mimport { SagaIterator } from "redux-saga"[m
[32m+[m[32mimport { take, delay, call, put } from "redux-saga/effects"[m
[32m+[m[32mimport * as actions from "../actions"[m
[32m+[m
[32m+[m[32mlet trackIndex = 0[m
[32m+[m[32mlet iterCount = 1[m
[32m+[m[32mlet pastPage = ""[m
[32m+[m[32mlet pageFilled = 0[m
[32m+[m[32mlet shufflePageChosenTracks: string[] = [][m
[32m+[m[32mlet shuffleSummary: string[][] = [][m
[32m+[m[32mlet shufflePages: any = {}[m
[32m+[m[32mlet isLoopTrue = true[m
[32m+[m
[32m+[m[32mconst getTracksArrSize = () => localStorage.getItem("tracksArrSize")[m
[32m+[m[32mconst getTracksPagesCount = () => localStorage.getItem("tracksPagesCount")[m
[32m+[m[32m// const getStopStatus = () => localStorage.getItem("stopLoopedPlay")[m
[32m+[m[32mconst setIsLoopTrue = (val: boolean) => (isLoopTrue = val)[m
[32m+[m
[32m+[m[32mconst checkObjIncludesNextArr = (obj: any, index: string) => {[m
[32m+[m[32m  let result = false[m
[32m+[m[32m  for (let key in obj) {[m
[32m+[m[32m    if (key === index) result = true[m
[32m+[m[32m  }[m
[32m+[m[32m  return result[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mconst chooseRandomPage = ([m
[32m+[m[32m  currPage: string,[m
[32m+[m[32m  maxPageCount: number,[m
[32m+[m[32m  arr: string[][],[m
[32m+[m[32m  maxPageArrSize: number,[m
[32m+[m[32m  obj: any,[m
[32m+[m[32m  pageFilledCount: number[m
[32m+[m[32m) => {[m
[32m+[m[32m  let allowPagesArr: number[] = [][m
[32m+[m[32m  let extraArr: number[] = [][m
[32m+[m
[32m+[m[32m  if (pageFilledCount) {[m
[32m+[m[32m    for (let count = 0; count < maxPageCount - pageFilledCount; count++) {[m
[32m+[m[32m      if (count !== +currPage) {[m
[32m+[m[32m        if (arr[count]?.length !== maxPageArrSize) {[m
[32m+[m[32m          allowPagesArr.push(count)[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  } else {[m
[32m+[m[32m    for (let count = 0; count < maxPageCount; count++) {[m
[32m+[m[32m      if (count !== +currPage) {[m
[32m+[m[32m        if (obj[`${count}`]) {[m
[32m+[m[32m          if (arr[count]?.length !== obj[`${count}`].length) {[m
[32m+[m[32m            if (count === maxPageCount - 1) {[m
[32m+[m[32m              allowPagesArr.push(count)[m
[32m+[m[32m            } else {[m
[32m+[m[32m              allowPagesArr.push(count)[m
[32m+[m[32m            }[m
[32m+[m[32m          }[m
[32m+[m[32m        } else {[m
[32m+[m[32m          if (arr[count]?.length !== maxPageArrSize) {[m
[32m+[m[32m            allowPagesArr.push(count)[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  // console.log("allowPagesArr", allowPagesArr)[m
[32m+[m
[32m+[m[32m  if (!allowPagesArr.length) {[m
[32m+[m[32m    if (pageFilledCount) {[m
[32m+[m[32m      for (let count = 0; count < maxPageCount - pageFilledCount; count++) {[m
[32m+[m[32m        if (arr[count]?.length !== maxPageArrSize) {[m
[32m+[m[32m          extraArr.push(count)[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    } else {[m
[32m+[m[32m      for (let count = 0; count < maxPageCount; count++) {[m
[32m+[m[32m        if (obj[`${count}`]) {[m
[32m+[m[32m          if (arr[count]?.length !== obj[`${count}`].length) {[m
[32m+[m[32m            if (count === maxPageCount - 1) {[m
[32m+[m[32m              extraArr.push(count)[m
[32m+[m[32m            } else {[m
[32m+[m[32m              extraArr.push(count)[m
[32m+[m[32m            }[m
[32m+[m[32m          }[m
[32m+[m[32m        } else {[m
[32m+[m[32m          if (arr[count]?.length !== maxPageArrSize) {[m
[32m+[m[32m            extraArr.push(count)[m
[32m+[m[32m          }[m
[32m+[m[32m        }[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  let nextPageToPlay =[m
[32m+[m[32m    allowPagesArr[Math.ceil(Math.random() * (allowPagesArr.length - 1))][m
[32m+[m
[32m+[m[32m  let extraPageToPlay =[m
[32m+[m[32m    extraArr[Math.ceil(Math.random() * (extraArr.length - 1))][m
[32m+[m
[32m+[m[32m  if (allowPagesArr.length) {[m
[32m+[m[32m    return nextPageToPlay[m
[32m+[m[32m  } else {[m
[32m+[m[32m    return extraArr.length ? extraPageToPlay : true[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mconst chooseRandomTrack = ([m
[32m+[m[32m  arr: HTMLDivElement[],[m
[32m+[m[32m  exceptionArr?: string[] | undefined[m
[32m+[m[32m) => {[m
[32m+[m[32m  // console.log("arr", arr)[m
[32m+[m[32m  // console.log("exceptionArr", exceptionArr)[m
[32m+[m
[32m+[m[32m  let indexArr: number[] = [][m
[32m+[m[32m  let allowArrIndex: number[] = [][m
[32m+[m[32m  let allowArr: number[] = [][m
[32m+[m[32m  let disallowArr: number[] = [][m
[32m+[m[32m  let allowNameArr: Array<string | undefined | null> = [][m
[32m+[m
[32m+[m[32m  for (let key in arr) {[m
[32m+[m[32m    indexArr.push(+key)[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  if (exceptionArr?.length) {[m
[32m+[m[32m    for (let item of exceptionArr) {[m
[32m+[m[32m      let ind = arr.findIndex((node) => node.textContent === item)[m
[32m+[m[32m      if (ind !== -1) {[m
[32m+[m[32m        disallowArr.push(ind)[m
[32m+[m[32m      }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    allowArrIndex = indexArr.filter((el) => !disallowArr.includes(el))[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  for (let i = 0; i < arr.length; i++) {[m
[32m+[m[32m    if (arr[i] !== null) {[m
[32m+[m[32m      allowArr.push(i)[m
[32m+[m[32m      allowNameArr.push(arr[i].textContent ? arr[i].textContent : undefined)[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  let nextTrackIndexToPlay =[m
[32m+[m[32m    allowArrIndex[Math.ceil(Math.random() * (allowArrIndex.length - 1))][m
[32m+[m
[32m+[m[32m  let nextTrackIndex =[m
[32m+[m[32m    allowArr[Math.ceil(Math.random() * (allowArr.length - 1))][m
[32m+[m
[32m+[m[32m  let origin =[m
[32m+[m[32m    allowNameArr[exceptionArr?.length ? nextTrackIndexToPlay : nextTrackIndex][m
[32m+[m
[32m+[m[32m  // console.log("allowArr", allowArr)[m
[32m+[m[32m  // console.log("disallowArr", disallowArr)[m
[32m+[m[32m  // console.log("allowArrIndex", allowArrIndex)[m
[32m+[m[32m  // console.log("allowNameArr", allowNameArr)[m
[32m+[m[32m  // console.log("nextTrackIndexToPlay", nextTrackIndexToPlay)[m
[32m+[m[32m  // console.log("nextTrackIndex", nextTrackIndex)[m
[32m+[m[32m  // console.log("origin", origin)[m
[32m+[m
[32m+[m[32m  if (disallowArr.length) {[m
[32m+[m[32m    return { i: nextTrackIndexToPlay, origin }[m
[32m+[m[32m  } else {[m
[32m+[m[32m    return { i: nextTrackIndex, origin }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mexport function* setTrackPlayStateSaga(): SagaIterator {[m
[32m+[m[32m  while (true) {[m
[32m+[m[32m    try {[m
[32m+[m[32m      const { payload: playState } = yield take(actions.setPlayStateAction)[m
[32m+[m[32m      yield put(actions.setPlayStateAction(playState))[m
[32m+[m[32m    } catch (e) {[m
[32m+[m[32m      throw new Error("setTrackStateAction failure!")[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mexport function* initializeShuffleSaga(): SagaIterator {[m
[32m+[m[32m  while (true) {[m
[32m+[m[32m    console.log("start initializeShuffleSaga")[m
[32m+[m[32m    try {[m
[32m+[m[32m      const { payload: playStatus } = yield take(actions.setTurnOnShufflePlay)[m
[32m+[m[32m      yield put(actions.setTurnOnShufflePlay(true))[m
[32m+[m[32m      if (playStatus) {[m
[32m+[m[32m        const { payload: arr } = yield take(actions.setTracksArrToConstantPlay)[m
[32m+[m
[32m+[m[32m        const pagesCount: string = yield call(getTracksPagesCount)[m
[32m+[m[32m        pastPage = pagesCount[m
[32m+[m[32m        const { i: trackIndex, origin: trackName } = yield call([m
[32m+[m[32m          chooseRandomTrack,[m
[32m+[m[32m          arr,[m
[32m+[m[32m          undefined[m
[32m+[m[32m        )[m
[32m+[m[32m        arr[trackIndex].click()[m
[32m+[m[32m        shufflePageChosenTracks = [][m
[32m+[m[32m        shufflePageChosenTracks.push(trackName)[m
[32m+[m[32m        shuffleSummary = [][m
[32m+[m[32m        shuffleSummary[+pagesCount] = shufflePageChosenTracks[m
[32m+[m
[32m+[m[32m        const indexToDel = arr.findIndex([m
[32m+[m[32m          (node: HTMLDivElement) => node === null[m
[32m+[m[32m        )[m
[32m+[m[32m        let copy = arr.slice(0, indexToDel)[m
[32m+[m[32m        shufflePages = {}[m
[32m+[m[32m        shufflePages[pagesCount] = indexToDel !== -1 ? [...copy] : [...arr][m
[32m+[m
[32m+[m[32m        const tracksArrSize = yield call(getTracksArrSize)[m
[32m+[m[32m        let maxPagesCount = Math.ceil(tracksArrSize / arr.length)[m
[32m+[m[32m        if ([m
[32m+[m[32m          shufflePageChosenTracks.length === shufflePages[pagesCount].length &&[m
[32m+[m[32m          +pagesCount === maxPagesCount - 1[m
[32m+[m[32m        ) {[m
[32m+[m[32m          pageFilled = pageFilled + 1[m
[32m+[m[32m        }[m
[32m+[m[32m        yield put(actions.setInitializeShuffleSuccess(true))[m
[32m+[m[32m      } else {[m
[32m+[m[32m        yield put(actions.setTurnOffShufflePlay(true))[m
[32m+[m[32m        yield put(actions.setTrackOrderToPlay(""))[m
[32m+[m[32m        // localStorage.setItem("stopLoopedPlay", "stop")[m
[32m+[m[32m        continue[m
[32m+[m[32m      }[m
[32m+[m[32m    } catch (e) {[m
[32m+[m[32m      throw new Error(`INITIALIZE Shuffle SAGA ERROR: ${e}`)[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mexport function* setShuffleToPlaySaga(): SagaIterator {[m
[32m+[m[32m  while (true) {[m
[32m+[m[32m    console.log("common SHUFFLE start")[m
[32m+[m[32m    try {[m
[32m+[m[32m      const { payload: isStartSuccess } = yield take([m
[32m+[m[32m        actions.setInitializeShuffleSuccess[m
[32m+[m[32m      )[m
[32m+[m[32m      setIsLoopTrue(true)[m
[32m+[m[32m      if (isStartSuccess) {[m
[32m+[m[32m        do {[m
[32m+[m[32m          const tracksArrSize = yield call(getTracksArrSize)[m
[32m+[m[32m          const pagesCount = yield call(getTracksPagesCount)[m
[32m+[m[32m          console.log("local SHUFFLE loop start")[m
[32m+[m[32m          const {[m
[32m+[m[32m            payload: playingStatus,[m
[32m+[m[32m            meta: {[m
[32m+[m[32m              i: currentTrackIndex,[m
[32m+[m[32m              buttons: { prevButton, nextButton },[m
[32m+[m[32m              arr,[m
[32m+[m[32m              closeBtnRef: { current: closeBtn },[m
[32m+[m[32m              ...rest[m
[32m+[m[32m            },[m
[32m+[m[32m          } = yield take(actions.setPlayingStatus)[m
[32m+[m
[32m+[m[32m          if (playingStatus === "closed") {[m
[32m+[m[32m            yield put(actions.setTurnOnShufflePlay(false))[m
[32m+[m[32m            setIsLoopTrue(false)[m
[32m+[m[32m            yield put(actions.setPlayingMode("single"))[m
[32m+[m[32m            let currPage = yield call(getTracksPagesCount)[m
[32m+[m[32m            yield put(actions.setTrackPage(+currPage))[m
[32m+[m[32m            yield put(actions.setPlayStateAction(""))[m
[32m+[m[32m            // localStorage.removeItem("stopLoopedPlay")[m
[32m+[m[32m            continue[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          if (playingStatus === "ended") {[m
[32m+[m[32m            let maxPagesCount = Math.ceil(tracksArrSize / arr.length)[m
[32m+[m
[32m+[m[32m            if (iterCount < arr.length) {[m
[32m+[m[32m              closeBtn.click()[m
[32m+[m[32m              yield take(actions.setShowPlaylistTracks)[m
[32m+[m
[32m+[m[32m              if (+pagesCount === 0) {[m
[32m+[m[32m                for ([m
[32m+[m[32m                  let i = 0;[m
[32m+[m[32m                  i <[m
[32m+[m[32m                  Math.ceil([m
[32m+[m[32m                    Math.random() *[m
[32m+[m[32m                      (maxPagesCount - 1 - (pageFilled ? pageFilled : 0))[m
[32m+[m[32m                  );[m
[32m+[m[32m                  i++[m
[32m+[m[32m                ) {[m
[32m+[m[32m                  nextButton.click()[m
[32m+[m[32m                }[m
[32m+[m[32m              } else if (+pagesCount === maxPagesCount - 1) {[m
[32m+[m[32m                for ([m
[32m+[m[32m                  let i = 0;[m
[32m+[m[32m                  i < Math.ceil(Math.random() * maxPagesCount);[m
[32m+[m[32m                  i++[m
[32m+[m[32m                ) {[m
[32m+[m[32m                  prevButton.click()[m
[32m+[m[32m                }[m
[32m+[m[32m              } else {[m
[32m+[m[32m                if (Math.random() > 0.5) {[m
[32m+[m[32m                  if (!pageFilled) {[m
[32m+[m[32m                    nextButton.click()[m
[32m+[m[32m                  } else {[m
[32m+[m[32m                    prevButton.click()[m
[32m+[m[32m                  }[m
[32m+[m[32m                } else {[m
[32m+[m[32m                  prevButton.click()[m
[32m+[m[32m                }[m
[32m+[m[32m              }[m
[32m+[m
[32m+[m[32m              yield delay(150)[m
[32m+[m[32m              let currPage = yield call(getTracksPagesCount)[m
[32m+[m[32m              yield put(actions.getNewShuffleTracksPageArr(currPage))[m
[32m+[m
[32m+[m[32m              const { payload: newArr } = yield take([m
[32m+[m[32m                actions.setNewShuffleTracksPageArr[m
[32m+[m[32m              )[m
[32m+[m
[32m+[m[32m              const isObjHasArr = yield call([m
[32m+[m[32m                checkObjIncludesNextArr,[m
[32m+[m[32m                shufflePages,[m
[32m+[m[32m                currPage[m
[32m+[m[32m              )[m
[32m+[m[32m              if (!isObjHasArr) {[m
[32m+[m[32m                const indexToDel = newArr.findIndex([m
[32m+[m[32m                  (node: HTMLDivElement) => node === null[m
[32m+[m[32m                )[m
[32m+[m
[32m+[m[32m                let copy = newArr.slice(0, indexToDel)[m
[32m+[m[32m                shufflePages[currPage] =[m
[32m+[m[32m                  indexToDel !== -1 ? [...copy] : [...newArr][m
[32m+[m[32m              }[m
[32m+[m
[32m+[m[32m              yield put(actions.setAutoPlayingStatus())[m
[32m+[m[32m              yield delay(50)[m
[32m+[m
[32m+[m[32m              const indexToDel = newArr.findIndex([m
[32m+[m[32m                (node: HTMLDivElement) => node === null[m
[32m+[m[32m              )[m
[32m+[m[32m              let copy = newArr.slice(0, indexToDel)[m
[32m+[m
[32m+[m[32m              const { i: nextIndex, origin: trackName } = yield call([m
[32m+[m[32m                chooseRandomTrack,[m
[32m+[m[32m                shufflePages[currPage] ? shufflePages[currPage] : [...copy],[m
[32m+[m[32m                shuffleSummary[+currPage]?.length[m
[32m+[m[32m                  ? shuffleSummary[+currPage][m
[32m+[m[32m                  : undefined[m
[32m+[m[32m              )[m
[32m+[m
[32m+[m[32m              newArr[nextIndex].click()[m
[32m+[m[32m              yield delay(50)[m
[32m+[m[32m              rest.playBtn.current.click()[m
[32m+[m
[32m+[m[32m              if (currPage !== pastPage) {[m
[32m+[m[32m                if (shuffleSummary[+currPage]?.length) {[m
[32m+[m[32m                  shufflePageChosenTracks = shuffleSummary[+currPage][m
[32m+[m[32m                } else {[m
[32m+[m[32m                  shufflePageChosenTracks = [][m
[32m+[m[32m                }[m
[32m+[m[32m              }[m
[32m+[m[32m              shufflePageChosenTracks.push(trackName)[m
[32m+[m
[32m+[m[32m              if ([m
[32m+[m[32m                shufflePageChosenTracks.length ===[m
[32m+[m[32m                  shufflePages[currPage].length &&[m
[32m+[m[32m                +currPage === maxPagesCount - 1[m
[32m+[m[32m              ) {[m
[32m+[m[32m                pageFilled = pageFilled + 1[m
[32m+[m[32m              }[m
[32m+[m[32m              shuffleSummary[+currPage] = shufflePageChosenTracks[m
[32m+[m
[32m+[m[32m              pastPage = currPage[m
[32m+[m[32m              iterCount++[m
[32m+[m[32m              continue[m
[32m+[m[32m            }[m
[32m+[m[32m            const nextPage = yield call([m
[32m+[m[32m              chooseRandomPage,[m
[32m+[m[32m              pagesCount,[m
[32m+[m[32m              maxPagesCount,[m
[32m+[m[32m              shuffleSummary,[m
[32m+[m[32m              arr.length,[m
[32m+[m[32m              shufflePages,[m
[32m+[m[32m              pageFilled[m
[32m+[m[32m            )[m
[32m+[m[32m            // console.log("nextPage", nextPage)[m
[32m+[m
[32m+[m[32m            if (nextPage === true) {[m
[32m+[m[32m              iterCount = 0[m
[32m+[m[32m              pastPage = ""[m
[32m+[m[32m              pageFilled = 0[m
[32m+[m[32m              shufflePageChosenTracks = [][m
[32m+[m[32m              shuffleSummary = [][m
[32m+[m[32m              shufflePages = {}[m
[32m+[m
[32m+[m[32m              closeBtn.click()[m
[32m+[m[32m              yield take(actions.setShowPlaylistTracks)[m
[32m+[m[32m              let currPage = yield call(getTracksPagesCount)[m
[32m+[m[32m              for (let i = 0; i < +currPage; i++) {[m
[32m+[m[32m                prevButton.click()[m
[32m+[m[32m                yield delay(50)[m
[32m+[m[32m              }[m
[32m+[m[32m              yield put(actions.getNewShuffleTracksPageArr(`${nextPage}`))[m
[32m+[m[32m              const { payload: newArr } = yield take([m
[32m+[m[32m                actions.setNewShuffleTracksPageArr[m
[32m+[m[32m              )[m
[32m+[m
[32m+[m[32m              yield delay(50)[m
[32m+[m[32m              yield put(actions.setAutoPlayingStatus())[m
[32m+[m[32m              yield delay(50)[m
[32m+[m[32m              currPage = yield call(getTracksPagesCount)[m
[32m+[m[32m              const indexToDel = newArr.findIndex([m
[32m+[m[32m                (node: HTMLDivElement) => node === null[m
[32m+[m[32m              )[m
[32m+[m[32m              let copy = newArr.slice(0, indexToDel)[m
[32m+[m[32m              shufflePages[currPage] =[m
[32m+[m[32m                indexToDel !== -1 ? [...copy] : [...newArr][m
[32m+[m
[32m+[m[32m              const { i: nextIndex, origin: trackName } = yield call([m
[32m+[m[32m                chooseRandomTrack,[m
[32m+[m[32m                shufflePages[currPage] ? shufflePages[currPage] : [...copy],[m
[32m+[m[32m                shuffleSummary[+currPage]?.length[m
[32m+[m[32m                  ? shuffleSummary[+currPage][m
[32m+[m[32m                  : undefined[m
[32m+[m[32m              )[m
[32m+[m
[32m+[m[32m              newArr[nextIndex].click()[m
[32m+[m[32m              yield delay(50)[m
[32m+[m[32m              rest.playBtn.current.click()[m
[32m+[m
[32m+[m[32m              shufflePageChosenTracks.push(trackName)[m
[32m+[m[32m              shuffleSummary[+currPage] = shufflePageChosenTracks[m
[32m+[m
[32m+[m[32m              // console.log("nextPage", nextPage)[m
[32m+[m[32m              // console.log("nextIndex", nextIndex)[m
[32m+[m[32m              // console.log("pagesCount", pagesCount)[m
[32m+[m[32m              // console.log("shufflePages", shufflePages)[m
[32m+[m[32m              // console.log("shuffleSummary", shuffleSummary)[m
[32m+[m[32m              continue[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            closeBtn.click()[m
[32m+[m[32m            yield take(actions.setShowPlaylistTracks)[m
[32m+[m
[32m+[m[32m            switch (pagesCount) {[m
[32m+[m[32m              case "0":[m
[32m+[m[32m                if (nextPage === 0) {[m
[32m+[m[32m                  pastPage = pagesCount[m
[32m+[m[32m                }[m
[32m+[m[32m                for (let i = 0; i < nextPage; i++) {[m
[32m+[m[32m                  pastPage = pagesCount[m
[32m+[m[32m                  nextButton.click()[m
[32m+[m[32m                  yield delay(100)[m
[32m+[m[32m                }[m
[32m+[m[32m                break[m
[32m+[m[32m              case "1":[m
[32m+[m[32m                switch (nextPage) {[m
[32m+[m[32m                  case 0:[m
[32m+[m[32m                    for (let i = 0; i < +pagesCount; i++) {[m
[32m+[m[32m                      pastPage = pagesCount[m
[32m+[m[32m                      prevButton.click()[m
[32m+[m[32m                      yield delay(100)[m
[32m+[m[32m                    }[m
[32m+[m[32m                    break[m
[32m+[m[32m                  case 1:[m
[32m+[m[32m                    pastPage = pagesCount[m
[32m+[m[32m                    break[m
[32m+[m[32m                  case 2:[m
[32m+[m[32m                    for (let i = +pagesCount; i < nextPage; i++) {[m
[32m+[m[32m                      pastPage = pagesCount[m
[32m+[m[32m                      nextButton.click()[m
[32m+[m[32m                      yield delay(100)[m
[32m+[m[32m                    }[m
[32m+[m[32m                    break[m
[32m+[m[32m                  case 3:[m
[32m+[m[32m                    for (let i = +pagesCount; i < nextPage; i++) {[m
[32m+[m[32m                      nextButton.click()[m
[32m+[m[32m                      yield delay(100)[m
[32m+[m[32m                    }[m
[32m+[m[32m                    break[m
[32m+[m[32m                }[m
[32m+[m[32m                break[m
[32m+[m[32m              case "2":[m
[32m+[m[32m                switch (nextPage) {[m
[32m+[m[32m                  case 0:[m
[32m+[m[32m                    for (let i = nextPage; i < +pagesCount; i++) {[m
[32m+[m[32m                      pastPage = pagesCount[m
[32m+[m[32m                      prevButton.click()[m
[32m+[m[32m                      yield delay(100)[m
[32m+[m[32m                    }[m
[32m+[m[32m                    break[m
[32m+[m[32m                  case 1:[m
[32m+[m[32m                    for (let i = nextPage; i < +pagesCount; i++) {[m
[32m+[m[32m                      pastPage = pagesCount[m
[32m+[m[32m                      prevButton.click()[m
[32m+[m[32m                      yield delay(100)[m
[32m+[m[32m                    }[m
[32m+[m[32m                    break[m
[32m+[m[32m                  case 2:[m
[32m+[m[32m                    pastPage = pagesCount[m
[32m+[m[32m                    break[m
[32m+[m[32m                  case 3:[m
[32m+[m[32m                    for (let i = +pagesCount; i < nextPage; i++) {[m
[32m+[m[32m                      pastPage = pagesCount[m
[32m+[m[32m                      nextButton.click()[m
[32m+[m[32m                      yield delay(100)[m
[32m+[m[32m                    }[m
[32m+[m[32m                    break[m
[32m+[m[32m                }[m
[32m+[m[32m                break[m
[32m+[m[32m              case "3":[m
[32m+[m[32m                if (nextPage === 3) {[m
[32m+[m[32m                  pastPage = pagesCount[m
[32m+[m[32m                }[m
[32m+[m[32m                for (let i = nextPage; i < +pagesCount; i++) {[m
[32m+[m[32m                  prevButton.click()[m
[32m+[m[32m                  yield delay(100)[m
[32m+[m[32m                }[m
[32m+[m[32m                break[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            yield put(actions.getNewShuffleTracksPageArr(`${nextPage}`))[m
[32m+[m[32m            const { payload: newArr } = yield take([m
[32m+[m[32m              actions.setNewShuffleTracksPageArr[m
[32m+[m[32m            )[m
[32m+[m[32m            let currPage = yield call(getTracksPagesCount)[m
[32m+[m
[32m+[m[32m            const isObjHasArr = yield call([m
[32m+[m[32m              checkObjIncludesNextArr,[m
[32m+[m[32m              shufflePages,[m
[32m+[m[32m              currPage[m
[32m+[m[32m            )[m
[32m+[m[32m            if (!isObjHasArr) {[m
[32m+[m[32m              const indexToDel = newArr.findIndex([m
[32m+[m[32m                (node: HTMLDivElement) => node === null[m
[32m+[m[32m              )[m
[32m+[m
[32m+[m[32m              let copy = newArr.slice(0, indexToDel)[m
[32m+[m[32m              shufflePages[currPage] =[m
[32m+[m[32m                indexToDel !== -1 ? [...copy] : [...newArr][m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            yield delay(50)[m
[32m+[m[32m            yield put(actions.setAutoPlayingStatus())[m
[32m+[m[32m            yield delay(50)[m
[32m+[m
[32m+[m[32m            const indexToDel = newArr.findIndex([m
[32m+[m[32m              (node: HTMLDivElement) => node === null[m
[32m+[m[32m            )[m
[32m+[m[32m            let copy = newArr.slice(0, indexToDel)[m
[32m+[m[32m            const { i: nextIndex, origin: trackName } = yield call([m
[32m+[m[32m              chooseRandomTrack,[m
[32m+[m[32m              shufflePages[nextPage] ? shufflePages[nextPage] : [...copy],[m
[32m+[m[32m              shuffleSummary[nextPage]?.length[m
[32m+[m[32m                ? shuffleSummary[nextPage][m
[32m+[m[32m                : undefined[m
[32m+[m[32m            )[m
[32m+[m
[32m+[m[32m            newArr[nextIndex].click()[m
[32m+[m[32m            yield delay(50)[m
[32m+[m[32m            rest.playBtn.current.click()[m
[32m+[m
[32m+[m[32m            // console.log([m
[32m+[m[32m            //   "pagesCount, pastPage, currPage, nextPage",[m
[32m+[m[32m            //   +pagesCount,[m
[32m+[m[32m            //   +pastPage,[m
[32m+[m[32m            //   +currPage,[m
[32m+[m[32m            //   nextPage[m
[32m+[m[32m            // )[m
[32m+[m
[32m+[m[32m            if (shuffleSummary[+currPage]?.length) {[m
[32m+[m[32m              shufflePageChosenTracks = shuffleSummary[+currPage][m
[32m+[m[32m            } else {[m
[32m+[m[32m              shufflePageChosenTracks = [][m
[32m+[m[32m            }[m
[32m+[m[32m            shufflePageChosenTracks.push(trackName)[m
[32m+[m
[32m+[m[32m            if ([m
[32m+[m[32m              shufflePageChosenTracks.length ===[m
[32m+[m[32m                shufflePages[currPage].length &&[m
[32m+[m[32m              +currPage === maxPagesCount - 1[m
[32m+[m[32m            ) {[m
[32m+[m[32m              pageFilled = pageFilled + 1[m
[32m+[m[32m            }[m
[32m+[m[32m            shuffleSummary[+currPage] = shufflePageChosenTracks[m
[32m+[m
[32m+[m[32m            // console.log("nextPage", nextPage)[m
[32m+[m[32m            // console.log("nextIndex", nextIndex)[m
[32m+[m[32m            // console.log("pagesCount", pagesCount)[m
[32m+[m[32m            // console.log("shufflePages", shufflePages)[m
[32m+[m[32m            // console.log("shuffleSummary", shuffleSummary)[m
[32m+[m[32m            pastPage = currPage[m
[32m+[m[32m            continue[m
[32m+[m[32m          }[m
[32m+[m[32m        } while (isLoopTrue)[m
[32m+[m[32m        console.log("loop end SHUFFLE")[m
[32m+[m[32m      }[m
[32m+[m[32m    } catch (e) {[m
[32m+[m[32m      throw new Error(`КАКАЯ-ТО ХРЕНЬ В SHUFFLE: ${e}`)[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mexport function* initializeSaga(): SagaIterator {[m
[32m+[m[32m  while (true) {[m
[32m+[m[32m    try {[m
[32m+[m[32m      const { payload: playStatus } = yield take(actions.setTurnOnTracksPlay)[m
[32m+[m[32m      yield put(actions.setTurnOnTracksPlay(true))[m
[32m+[m
[32m+[m[32m      if (playStatus) {[m
[32m+[m[32m        const { payload: arr } = yield take(actions.setTracksArrToConstantPlay)[m
[32m+[m[32m        arr[0].click()[m
[32m+[m
[32m+[m[32m        yield put(actions.setInitializePlaySuccess(true))[m
[32m+[m[32m      } else {[m
[32m+[m[32m        yield put(actions.setTurnOffTracksPlay(true))[m
[32m+[m[32m        yield put(actions.setTrackOrderToPlay(""))[m
[32m+[m
[32m+[m[32m        // localStorage.setItem("stopLoopedPlay", "stop")[m
[32m+[m[32m        continue[m
[32m+[m[32m      }[m
[32m+[m[32m    } catch (e) {[m
[32m+[m[32m      throw new Error(`INITIALIZE InOrder SAGA ERROR: ${e}`)[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mexport function* setPlaylistToPlaySaga(): SagaIterator {[m
[32m+[m[32m  while (true) {[m
[32m+[m[32m    console.log("common start")[m
[32m+[m[32m    try {[m
[32m+[m[32m      const { payload: isStartSuccess } = yield take([m
[32m+[m[32m        actions.setInitializePlaySuccess[m
[32m+[m[32m      )[m
[32m+[m[32m      setIsLoopTrue(true)[m
[32m+[m[32m      if (isStartSuccess) {[m
[32m+[m[32m        do {[m
[32m+[m[32m          const tracksArrSize = yield call(getTracksArrSize)[m
[32m+[m[32m          const pagesCount = yield call(getTracksPagesCount)[m
[32m+[m[32m          console.log("local loop start")[m
[32m+[m[32m          const {[m
[32m+[m[32m            payload: playingStatus,[m
[32m+[m[32m            meta: {[m
[32m+[m[32m              i: currentTrackIndex,[m
[32m+[m[32m              buttons: { prevButton, nextButton },[m
[32m+[m[32m              arr,[m
[32m+[m[32m              closeBtnRef: { current: closeBtn },[m
[32m+[m[32m              ...rest[m
[32m+[m[32m            },[m
[32m+[m[32m          } = yield take(actions.setPlayingStatus)[m
[32m+[m
[32m+[m[32m          if (playingStatus === "closed") {[m
[32m+[m[32m            console.log("!playingStatus")[m
[32m+[m[32m            yield put(actions.setTurnOnTracksPlay(false))[m
[32m+[m[32m            setIsLoopTrue(false)[m
[32m+[m[32m            yield put(actions.setPlayingMode("single"))[m
[32m+[m[32m            let currPage = yield call(getTracksPagesCount)[m
[32m+[m[32m            yield put(actions.setTrackPage(+currPage))[m
[32m+[m[32m            yield put(actions.setPlayStateAction(""))[m
[32m+[m[32m            // localStorage.removeItem("stopLoopedPlay")[m
[32m+[m[32m            continue[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          // const isStoped = yield call(getStopStatus)[m
[32m+[m[32m          // if (isStoped === "stop") {[m
[32m+[m[32m          //   if (!playingStatus) {[m
[32m+[m[32m          //     console.log("stop + ended")[m
[32m+[m[32m          //     closeBtn.click()[m
[32m+[m[32m          //     yield take(actions.setShowPlaylistTracks)[m
[32m+[m[32m          //     yield delay(100)[m
[32m+[m[32m          //     localStorage.removeItem("stopLoopedPlay")[m
[32m+[m[32m          //     setIsLoopTrue(false)[m
[32m+[m[32m          //     console.log("!!")[m
[32m+[m[32m          //     continue[m
[32m+[m[32m          //   }[m
[32m+[m[32m          // }[m
[32m+[m
[32m+[m[32m          if (currentTrackIndex === 0) {[m
[32m+[m[32m            trackIndex = pagesCount * arr.length[m
[32m+[m[32m          }[m
[32m+[m
[32m+[m[32m          if (playingStatus === "ended") {[m
[32m+[m[32m            trackIndex = trackIndex + 1[m
[32m+[m
[32m+[m[32m            if (trackIndex < +tracksArrSize) {[m
[32m+[m[32m              closeBtn.click()[m
[32m+[m[32m              yield take(actions.setShowPlaylistTracks)[m
[32m+[m[32m              yield delay(100)[m
[32m+[m
[32m+[m[32m              if (currentTrackIndex === arr.length - 1) {[m
[32m+[m[32m                nextButton.click()[m
[32m+[m[32m                yield delay(50)[m
[32m+[m[32m                yield put(actions.setAutoPlayingStatus())[m
[32m+[m[32m                yield delay(50)[m
[32m+[m[32m                arr[0].click()[m
[32m+[m[32m                yield delay(50)[m
[32m+[m[32m                rest.playBtn.current.click()[m
[32m+[m[32m                continue[m
[32m+[m[32m              }[m
[32m+[m
[32m+[m[32m              yield put(actions.setAutoPlayingStatus())[m
[32m+[m[32m              yield delay(50)[m
[32m+[m[32m              arr[currentTrackIndex + 1].click()[m
[32m+[m[32m            } else {[m
[32m+[m[32m              closeBtn.click()[m
[32m+[m[32m              yield take(actions.setShowPlaylistTracks)[m
[32m+[m[32m              yield delay(100)[m
[32m+[m
[32m+[m[32m              for (let i = 0; i < pagesCount; i++) {[m
[32m+[m[32m                prevButton.click()[m
[32m+[m[32m                yield delay(50)[m
[32m+[m[32m              }[m
[32m+[m[32m              trackIndex = 0[m
[32m+[m[32m              yield put(actions.setAutoPlayingStatus())[m
[32m+[m[32m              yield delay(50)[m
[32m+[m[32m              arr[0].click()[m
[32m+[m[32m              yield delay(50)[m
[32m+[m[32m              rest.playBtn.current.click()[m
[32m+[m[32m            }[m
[32m+[m[32m          }[m
[32m+[m[32m        } while (isLoopTrue)[m
[32m+[m[32m        console.log("loop end")[m
[32m+[m[32m      }[m
[32m+[m[32m    } catch (e) {[m
[32m+[m[32m      throw new Error(`КАКАЯ-ТО ХРЕНЬ: ${e}`)[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m[32m}[m

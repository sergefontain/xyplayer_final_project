[32m+[m[32mimport { ActionType } from "typesafe-actions"[m
[32m+[m[32mimport * as actions from "../actions"[m
[32m+[m
[32m+[m[32mexport interface PlayerState {[m
[32m+[m[32m    volValue: number[m
[32m+[m[32m    currTime: number[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mexport type PlayerAction = ActionType<typeof actions>[m

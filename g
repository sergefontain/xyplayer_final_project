[1m+++ b/src/store/rootReducer.ts[m
[36m@@ -0,0 +1,16 @@[m
[32m+[m[32mimport { combineReducers } from "redux"[m
[32m+[m[32mimport {EmptyAction} from 'typesafe-actions'[m
[32m+[m[32mimport authReducer from "./auth"[m
[32m+[m[32mimport playerReducer from "./player"[m
[32m+[m[32mimport { PlayerAction } from "./player/types"[m
[32m+[m
[32m+[m[32mconst rootReducer = combineReducers({[m
[32m+[m[32m    auth: authReducer,[m
[32m+[m[32m    play: playerReducer[m
[32m+[m[32m})[m
[32m+[m[41m  [m
[32m+[m[41m  [m
[32m+[m[32mexport type RootState = ReturnType<typeof rootReducer>[m
[32m+[m[32mexport type RootAction = EmptyAction<string> | PlayerAction[m
[32m+[m[41m  [m
[32m+[m[32mexport default rootReducer[m

[1mdiff --git a/src/store/auth/index.ts b/src/store/auth/index.ts[m
[1mnew file mode 100644[m
[1mindex 0000000..0d63823[m
[1m--- /dev/null[m
[1m+++ b/src/store/auth/index.ts[m
[36m@@ -0,0 +1,24 @@[m
[32m+[m[32mimport { getType } from "typesafe-actions"[m
[32m+[m[32mimport * as actions from "../actions"[m
[32m+[m[32mimport { AuthAction, AuthState } from "./types"[m
[32m+[m
[32m+[m[32mconst initialState: AuthState = {[m
[32m+[m[32m  isLogon: false,[m
[32m+[m[32m  authStatus: "",[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mconst authReducer = ([m
[32m+[m[32m  state: AuthState = initialState,[m
[32m+[m[32m  action: AuthAction[m
[32m+[m[32m): AuthState => {[m
[32m+[m[32m  switch (action.type) {[m
[32m+[m[32m    case getType(actions.authSuccess):[m
[32m+[m[32m      return { ...state, authStatus: "success", isLogon: true }[m
[32m+[m[32m    case getType(actions.authFailure):[m
[32m+[m[32m      return { ...state, authStatus: "failure", isLogon: false }[m
[32m+[m[32m    default:[m
[32m+[m[32m      return state[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mexport default authReducer[m
\ No newline at end of file[m

[36m@@ -0,0 +1,347 @@[m
[32m+[m[32mimport React, { useState } from "react"[m
[32m+[m[32mimport { Row, Spinner, Col, Alert, Button, Container } from "react-bootstrap"[m
[32m+[m[32mimport { connect } from "react-redux"[m
[32m+[m[32mimport { bindActionCreators, Dispatch } from "redux"[m
[32m+[m[32mimport { PayloadActionCreator } from "typesafe-actions"[m
[32m+[m[32mimport { PlaylistsFind, TracksFind } from "../../store/main/types"[m
[32m+[m[32mimport Player from "../AudioPlayer"[m
[32m+[m[32mimport { RootAction, RootState } from "../../store/rootReducer"[m
[32m+[m[32mimport * as actions from "./../../store/actions"[m
[32m+[m[32mimport { CSSTransition } from "react-transition-group"[m
[32m+[m[32mimport "./styles.css"[m
[32m+[m
[32m+[m[32minterface PlaylistResolver {[m
[32m+[m[32m  queryStatus: string[m
[32m+[m[32m  playlistsFromRedux: PlaylistsFind | null[m
[32m+[m[32m  getTracks: PayloadActionCreator<"main/GET_TRACKS_REQUEST", string>[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32minterface TrackResolver {[m
[32m+[m[32m  queryStatus: string[m
[32m+[m[32m  tracksFromRedux: TracksFind | null[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mtype Props = ReturnType<typeof mapStateToProps> &[m
[32m+[m[32m  ReturnType<typeof mapDispatchToProps>[m
[32m+[m
[32m+[m[32mconst mapStateToProps = (state: RootState) => ({[m
[32m+[m[32m  playlistsFromRedux: state.main.playlists,[m
[32m+[m[32m  queryStatus: state.main.queryStatus,[m
[32m+[m[32m  tracksFromRedux: state.main.tracks,[m
[32m+[m[32m  currentPlaylistPage: state.main.currentPlaylistPage,[m
[32m+[m[32m  pageLimitOverload: state.main.pageLimitOverload,[m
[32m+[m[32m})[m
[32m+[m
[32m+[m[32mconst mapDispatchToProps = (dispatch: Dispatch<RootAction>) =>[m
[32m+[m[32m  bindActionCreators([m
[32m+[m[32m    {[m
[32m+[m[32m      getTracks: actions.getTracksReq,[m
[32m+[m[32m      prevPlaylistPage: actions.prevPlaylistPage,[m
[32m+[m[32m      nextPlaylistPage: actions.nextPlaylistPage,[m
[32m+[m[32m    },[m
[32m+[m[32m    dispatch[m
[32m+[m[32m  )[m
[32m+[m
[32m+[m[32mconst GetPlaylists: React.FC<Props> = ({[m
[32m+[m[32m  playlistsFromRedux,[m
[32m+[m[32m  queryStatus,[m
[32m+[m[32m  tracksFromRedux,[m
[32m+[m[32m  getTracks,[m
[32m+[m[32m  prevPlaylistPage,[m
[32m+[m[32m  nextPlaylistPage,[m
[32m+[m[32m  currentPlaylistPage,[m
[32m+[m[32m  pageLimitOverload,[m
[32m+[m[32m}) => {[m
[32m+[m[32m  const [showButton, setShowButton] = useState(true)[m
[32m+[m[32m  const [showMessage, setShowMessage] = useState("")[m
[32m+[m
[32m+[m[32m  const MapPlaylistsQuery = ({[m
[32m+[m[32m    queryStatus,[m
[32m+[m[32m    playlistsFromRedux,[m
[32m+[m[32m    getTracks,[m
[32m+[m[32m  }: PlaylistResolver) => {[m
[32m+[m[32m    if (queryStatus === "idle") {[m
[32m+[m[32m      return <span className="my-3">Playlists Search Data</span>[m
[32m+[m[32m    }[m
[32m+[m[32m    if (queryStatus === "playlists_pending") {[m
[32m+[m[32m      return <Spinner animation="grow" />[m
[32m+[m[32m    }[m
[32m+[m[32m    if ([m
[32m+[m[32m      queryStatus === "playlists_succeed" ||[m
[32m+[m[32m      queryStatus === "tracks_pending" ||[m
[32m+[m[32m      queryStatus === "tracks_succeed"[m
[32m+[m[32m    ) {[m
[32m+[m[32m      if (playlistsFromRedux!.PlaylistFind) {[m
[32m+[m[32m        let filteredPlaylist = playlistsFromRedux!.PlaylistFind.filter((z) => {[m
[32m+[m[32m          var mySet = new Set(z.tracks)[m
[32m+[m[32m          if (mySet.size !== 0) {[m
[32m+[m[32m            return z[m
[32m+[m[32m          }[m
[32m+[m[32m        })[m
[32m+[m[32m        return ([m
[32m+[m[32m          <>[m
[32m+[m[32m            {filteredPlaylist.map((x, i) => {[m
[32m+[m[32m              if (x["name"] !== "" && x["name"] !== null) {[m
[32m+[m[32m                return ([m
[32m+[m[32m                  <div[m
[32m+[m[32m                    key={i.toString()}[m
[32m+[m[32m                    onClick={() => getTracks(x._id)}[m
[32m+[m[32m                    className="mappedPlaylist"[m
[32m+[m[32m                  >[m
[32m+[m[32m                    {x.name}[m
[32m+[m[32m                  </div>[m
[32m+[m[32m                )[m
[32m+[m[32m              } else {[m
[32m+[m[32m                return null[m
[32m+[m[32m              }[m
[32m+[m[32m            })}[m
[32m+[m[32m          </>[m
[32m+[m[32m        )[m
[32m+[m[32m      } else {[m
[32m+[m[32m        return <div>No Data yet</div>[m
[32m+[m[32m      }[m
[32m+[m[32m    } else {[m
[32m+[m[32m      return <div>unknown error!</div>[m
[32m+[m[32m    }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  const MapTracksQuery = ({ queryStatus, tracksFromRedux }: TrackResolver) => {[m
[32m+[m[32m    if ([m
[32m+[m[32m      queryStatus === "playlists_succeed" ||[m
[32m+[m[32m      queryStatus === "idle" ||[m
[32m+[m[32m      queryStatus === "playlists_pending"[m
[32m+[m[32m    ) {[m
[32m+[m[32m      return <div>Tracks Search Data</div>[m
[32m+[m[32m    }[m
[32m+[m[32m    if (queryStatus === "tracks_pending") {[m
[32m+[m[32m      return <Spinner animation="grow" />[m
[32m+[m[32m    }[m
[32m+[m[32m    if (queryStatus === "tracks_succeed") {[m
[32m+[m[32m      if (!tracksFromRedux) {[m
[32m+[m[32m        return null[m
[32m+[m[32m      } else if (tracksFromRedux!.PlaylistFindOne) {[m
[32m+[m[32m        let filteredPlaylist = tracksFromRedux!.PlaylistFindOne.tracks.filter([m
[32m+[m[32m          (z) => z.url[m
[32m+[m[32m        )[m
[32m+[m[32m        return ([m
[32m+[m[32m          <Container fluid className="flex-grow-1 d-flex flex-column">[m
[32m+[m[32m            {filteredPlaylist.map((x, i) => {[m
[32m+[m[32m              return ([m
[32m+[m[32m                <Row key={i.toString()} className="mappedSong flex-column">[m
[32m+[m[32m                  {showButton && ([m
[32m+[m[32m                    <Col onClick={() => setShowMessage(x._id)} className="">[m
[32m+[m[32m                      {x.originalFileName}[m
[32m+[m[32m                    </Col>[m
[32m+[m[32m                  )}[m
[32m+[m
[32m+[m[32m                  <CSSTransition[m

[32m+[m[32mimport { getType } from "typesafe-actions"[m
[32m+[m[32mimport { PlayerAction, PlayerState } from "./types"[m
[32m+[m[32mimport * as actions from "../actions"[m
[32m+[m
[32m+[m[32mconst initialState: PlayerState = {[m
[32m+[m[32m  volValue: 0,[m
[32m+[m[32m  currTime: 0[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mconst playerReducer = ([m
[32m+[m[32m  state: PlayerState = initialState,[m
[32m+[m[32m  action: PlayerAction[m
[32m+[m[32m): PlayerState => {[m
[32m+[m[32m  switch (action.type) {[m
[32m+[m[32m    case getType(actions.savingVolValue):[m
[32m+[m[32m      return { ...state, volValue: action.payload }[m
[32m+[m[32m      case getType(actions.savingCurrTime):[m
[32m+[m[32m      return { ...state, currTime: action.payload }[m
[32m+[m[32m    default:[m
[32m+[m[32m      return state[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mexport default playerReducer[m

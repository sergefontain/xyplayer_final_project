[32m+[m[32m  query: any,[m
[32m+[m[32m  variables: {},[m
[32m+[m[32m  params?: Params[m
[32m+[m[32m): Promise<any> => {[m
[32m+[m[32m  if (params) {[m
[32m+[m[32m    const response = await fetch(ENDPOINT_GRAPHQL, {[m
[32m+[m[32m      method: "POST",[m
[32m+[m[32m      body: JSON.stringify({[m
[32m+[m[32m        query,[m
[32m+[m[32m        variables,[m
[32m+[m[32m      }),[m
[32m+[m[32m      headers: {[m
[32m+[m[32m        "content-type": "application/json",[m
[32m+[m[32m        Authorization: params.headers.Authorization,[m
[32m+[m[32m      },[m
[32m+[m[32m    })[m
[32m+[m[32m    const { data } = await response.json()[m
[32m+[m[32m    return data[m
[32m+[m[32m  } else {[m
[32m+[m[32m    const response = await fetch(ENDPOINT_GRAPHQL, {[m
[32m+[m[32m      method: "POST",[m
[32m+[m[32m      body: JSON.stringify({[m
[32m+[m[32m        query,[m
[32m+[m[32m        variables,[m
[32m+[m[32m      }),[m
